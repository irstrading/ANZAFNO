PROJECT_NAME="ANZA FNO Intelligence Platform"
VERSION="1.0.0"
DEBUG=True
API_PREFIX="/api/v1"

# Database Configuration (PostgreSQL/TimescaleDB)
# Default for local Windows install: postgres user, password 'password', db 'anza_fno'
DATABASE_URL=postgresql://postgres:password@localhost:5432/anza_fno

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# AngelOne API Credentials
ANGEL_API_KEY=your_api_key_here
ANGEL_CLIENT_ID=your_client_id
ANGEL_PASSWORD=your_password
ANGEL_TOTP_KEY=your_totp_key

# OpenAlgo API Credentials (Optional)
OPENALGO_API_KEY=your_openalgo_key
OPENALGO_HOST=http://localhost:5000

# Fetch Intervals (seconds)
MARKET_DATA_INTERVAL=60
OI_ANALYSIS_INTERVAL=180
